<p id="notice"><%= notice %></p>

<h1>Your Leauges</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @leauges.each do |leauge| %>
      <tr>
        <td><%= leauge.name %></td>
        <td><%= link_to 'View', leauge %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Leauge', new_leauge_path %>


